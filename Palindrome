#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
    char str[20];
    int i,len,flag=0;
    printf("enter the string:");
    scanf("%s",str);
    len=strlen(str);
    for(i=0;i<len/2;i++)
    {
        if(str[i]==str[len-i-1])
        {
            flag=1;
            break;

        }

    }
    if(flag==1)
    {
        printf("string is palindrome");

    }
    else
    {
            printf("string is not palindrome");
    }
}
